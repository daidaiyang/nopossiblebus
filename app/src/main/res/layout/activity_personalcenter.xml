<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y599"
        android:background="@drawable/personnalcenter_back">

        <ImageView
            android:id="@+id/myhome"
            android:layout_width="@dimen/x46"
            android:layout_height="@dimen/y46"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y50"
            android:scaleType="centerInside"
            android:src="@drawable/home" />

        <ImageView
            android:id="@+id/mysetting"
            android:layout_width="@dimen/x46"
            android:layout_height="@dimen/y46"
            android:layout_alignParentEnd="true"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y50"
            android:layout_marginRight="@dimen/x144"
            android:scaleType="centerInside"
            android:src="@drawable/setting" />

        <FrameLayout
            android:id="@+id/mymessage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="@dimen/x40">

            <ImageView
                android:id="@+id/mymessage_img"
                android:layout_width="@dimen/x46"
                android:layout_height="@dimen/y46"
                android:layout_marginLeft="@dimen/x40"
                android:layout_marginTop="@dimen/y50"
                android:scaleType="centerInside"
                android:src="@drawable/message" />
            <View
                android:id="@+id/myssage_circle"
                android:layout_width="@dimen/x14"
                android:layout_height="@dimen/x14"
                android:layout_gravity="right|bottom"
                android:layout_marginBottom="@dimen/x40"
                android:background="@drawable/circle_point_x14_00"/>
        </FrameLayout>

        <ImageView
            android:id="@+id/touxiangImg"
            android:layout_width="@dimen/x156"
            android:layout_height="@dimen/y156"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginTop="@dimen/y220"/>

        <TextView
            android:id="@+id/nickName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x223"
            android:layout_marginTop="@dimen/y225"
            android:includeFontPadding="false"
            android:textColor="@color/text_black"
            android:textSize="@dimen/textSize63"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x225"
            android:layout_marginTop="@dimen/y318"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/x46"
                android:layout_height="@dimen/y46"
                android:src="@drawable/dian" />

            <ImageView
                android:layout_width="@dimen/x46"
                android:layout_height="@dimen/y46"
                android:layout_marginLeft="@dimen/x17"
                android:src="@drawable/gong_small" />

            <ImageView
                android:layout_width="@dimen/x151"
                android:layout_height="@dimen/y52"
                android:layout_marginLeft="@dimen/x17"
                android:src="@drawable/yirenzheng" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/y46"
                android:layout_marginLeft="@dimen/x17"
                android:background="@drawable/rect_circle_x9_0f"
                android:gravity="center"
                android:paddingLeft="@dimen/x16"
                android:paddingRight="@dimen/x16"
                android:text="2年"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize29" />
        </LinearLayout>

        <ImageView
            android:layout_width="@dimen/x30"
            android:layout_height="@dimen/x30"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="@dimen/y280"
            android:layout_marginRight="@dimen/x52"
            android:src="@drawable/jiantou_you" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/mywallet"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/mywallet" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="我的钱包"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <TextView
                    android:id="@+id/mywallet_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x44"
                    android:text="￥0.00"
                    android:textColor="@color/text_black_e0c" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x3"
                android:layout_marginLeft="@dimen/x58"
                android:layout_marginRight="@dimen/x58"
                android:background="@color/text_black_f2" />

            <RelativeLayout
                android:id="@+id/myintegral"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/myjifen" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="我的积分"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <TextView
                    android:id="@+id/myintegral_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x44"
                    android:text="0分"
                    android:textColor="@color/text_black_e0c" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x3"
                android:layout_marginLeft="@dimen/x58"
                android:layout_marginRight="@dimen/x58"
                android:background="@color/text_black_f2" />

            <RelativeLayout
                android:id="@+id/mykucun"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/mykucun" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="我的商品库存"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x3"
                android:layout_marginLeft="@dimen/x58"
                android:layout_marginRight="@dimen/x58"
                android:background="@color/text_black_f2" />

            <RelativeLayout
                android:id="@+id/myStatistics"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/mytongji" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="我的统计"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x3"
                android:layout_marginLeft="@dimen/x58"
                android:layout_marginRight="@dimen/x58"
                android:background="@color/text_black_f2" />

            <RelativeLayout
                android:id="@+id/myAddress"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/shouhuo_address" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="我的收货地址"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x3"
                android:layout_marginLeft="@dimen/x58"
                android:layout_marginRight="@dimen/x58"
                android:background="@color/text_black_f2" />

            <RelativeLayout
                android:id="@+id/share"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/tuijian" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="推荐有礼"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x3"
                android:layout_marginLeft="@dimen/x58"
                android:layout_marginRight="@dimen/x58"
                android:background="@color/text_black_f2" />

            <RelativeLayout
                android:id="@+id/helpcenter"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/helpcenter" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="帮助中心"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/x3"
                android:layout_marginLeft="@dimen/x58"
                android:layout_marginRight="@dimen/x58"
                android:background="@color/text_black_f2" />

            <RelativeLayout
                android:id="@+id/contract_us"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y173"
                android:layout_marginRight="@dimen/x58">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/y52"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x58"
                    android:src="@drawable/contract_us" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x150"
                    android:text="联系我们"
                    android:textColor="@color/text_black"
                    android:textSize="@dimen/textSize46" />

                <ImageView
                    android:layout_width="@dimen/x30"
                    android:layout_height="@dimen/y30"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/jiantou_wallet" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>